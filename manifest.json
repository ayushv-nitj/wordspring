{
  "manifest_version": 3,
  "name": "Word Spring",
  "version": "1.0.0",
    "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
 "description": "Save, learn, and revisit new words while you read.",  "permissions": ["storage", "alarms"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
